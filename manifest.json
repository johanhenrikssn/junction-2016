{
  "name": "Presens",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["clickListener.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": [
    "contextMenus",
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
